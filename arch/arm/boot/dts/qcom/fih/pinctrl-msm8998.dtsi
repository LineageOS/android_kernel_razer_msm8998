&soc {
	tlmm: pinctrl@03400000 {
		/* gpio 35 */ /* AUDIO_SW_EN */
		ear_switch_en1_idle: ear_switch_en1_idle {
			mux {
				pins = "gpio35";
				function = "gpio";
			};
			config {
				pins = "gpio35";
				drive-strength = <2>;
				bias-pull-down;
				output-low;
			};
		};

		ear_switch_en1_active: ear_switch_en1_active {
			mux {
				pins = "gpio35";
				function = "gpio";
			};
			config {
				pins = "gpio35";
				drive-strength = <2>;
				bias-disable;
				output-high;
			};
		};

		/* gpio 51 */ /* SMART_AMP_DAT */
		msm8998_gpio51_active: msm8998_gpio51_active {
			mux {
				pins = "gpio51";
				function = "blsp_i2c9";
			};

			config {
				pins = "gpio51";
				drive-strength = <2>;
				bias-disable;
			};
		};

		msm8998_gpio51_sleep: msm8998_gpio51_sleep {
			mux {
				pins = "gpio51";
				function = "blsp_i2c9";
			};

			config {
				pins = "gpio51";
				drive-strength = <2>;
				bias-disable;
			};
		};

		/* gpio 52 */ /* SMART_AMP_SCL */
		msm8998_gpio52_active: msm8998_gpio52_active {
			mux {
				pins = "gpio52";
				function = "blsp_i2c9";
			};

			config {
				pins = "gpio52";
				drive-strength = <2>;
				bias-disable;
			};
		};

		msm8998_gpio52_sleep: msm8998_gpio52_sleep {
			mux {
				pins = "gpio52";
				function = "blsp_i2c9";
			};

			config {
				pins = "gpio52";
				drive-strength = <2>;
				bias-disable;
			};
		};

		/* gpio 54 */ /* CODEC_INT1 */
		msm8998_gpio54_default: msm8998_gpio54_default{
			mux {
				pins = "gpio54";
				function = "gpio";
			};

			config {
				pins = "gpio54";
				drive-strength = <2>;
				bias-disable;
				input-enable;
			};
		};

		/* gpio 58 */ /* CDC_TX_I2S_SCK */
		msm8998_gpio58_active: msm8998_gpio58_active {
			mux {
				pins = "gpio58";
				function = "qua_mi2s";
			};

			config {
				pins = "gpio58";
				drive-strength = <8>;
				bias-disable;
				output-high;
			};
		};

		msm8998_gpio58_sleep: msm8998_gpio58_sleep {
			mux {
				pins = "gpio58";
				function = "qua_mi2s";
			};

			config {
				pins = "gpio58";
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		/* gpio 59 */ /* CDC_TX_I2S_WS */
		msm8998_gpio59_active: msm8998_gpio59_active {
			mux {
				pins = "gpio59";
				function = "qua_mi2s";
			};

			config {
				pins = "gpio59";
				drive-strength = <8>;
				bias-disable;
				output-high;
			};
		};

		msm8998_gpio59_sleep: msm8998_gpio59_sleep {
			mux {
				pins = "gpio59";
				function = "qua_mi2s";
			};

			config {
				pins = "gpio59";
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		/* gpio 60 */ /* CDC_TX_I2S_DI */
		msm8998_gpio60_active: msm8998_gpio60_active {
			mux {
				pins = "gpio60";
				function = "qua_mi2s";
			};

			config {
				pins = "gpio60";
				drive-strength = <8>;
				bias-disable;
			};
		};

		msm8998_gpio60_sleep: msm8998_gpio60_sleep {
			mux {
				pins = "gpio60";
				function = "qua_mi2s";
			};

			config {
				pins = "gpio60";
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		/* gpio 61 */ /* CDC_TX_I2S_DO */
		msm8998_gpio61_active: msm8998_gpio61_active {
			mux {
				pins = "gpio61";
				function = "qua_mi2s";
			};

			config {
				pins = "gpio61";
				drive-strength = <8>;
				bias-disable;
				input-enable;
			};
		};

		msm8998_gpio61_sleep: msm8998_gpio61_sleep {
			mux {
				pins = "gpio61";
				function = "qua_mi2s";
			};

			config {
				pins = "gpio61";
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		/* gpio 64 */ /* CODEC_RST_N */
		msm8998_gpio64_active:msm8998_gpio64_active {
			mux {
				pins = "gpio64";
				function = "gpio";
			};
			config {
				pins = "gpio64";
				drive-strength = <2>;
				bias-disable;
				output-high;
			};
		};

		msm8998_gpio64_sleep: msm8998_gpio64_sleep {
			mux {
				pins = "gpio64";
				function = "gpio";
			};
			config {
				pins = "gpio64";
				drive-strength = <2>;
				bias-disable;
				output-low;
			};
		};

		/* gpio 66 */ /* AUDIO_SW_IN */
		ear_switch_in1_idle: ear_switch_in1_idle {
			mux {
				pins = "gpio66";
				function = "gpio";
			};
			config {
				pins = "gpio66";
				drive-strength = <2>;
				bias-pull-down;
				output-low;
			};
		};

		ear_switch_in1_active: ear_switch_in1_active {
			mux {
				pins = "gpio66";
				function = "gpio";
			};
			config {
				pins = "gpio66";
				drive-strength = <2>;
				bias-disable;
				output-high;
			};
		};

		sdc2_cd_on: sdc2_cd_on {
			config {
				bias-disable;           /* no pull */
			};
		};

		sdc2_cd_off: sdc2_cd_off {
			config {
				bias-disable;           /* no pull */
			};
		};
	};
};
