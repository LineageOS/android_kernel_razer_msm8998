&soc {
	i2c@c1b7000 { /* BLSP2 QUP3 */
		status = "ok";

		tfa98xx@34 { /* Bottom */
			compatible = "nxp,tfa98xx";
			reg = <0x34>;
			reset-gpio = <&tlmm 49 0>;
			irq-gpio = <&tlmm 34 0>;
		};

		tfa98xx@35 { /* Top */
			compatible = "nxp,tfa98xx";
			reg = <0x35>;
		};
	};

	spi@c1b8000 { /* BLSP2 QUP4 */
		status = "disabled";
	};

	pinctrl@03400000 {
		wcd9xxx_intr {
			wcd_intr_default: wcd_intr_default {
				config {
					bias-disable;
				};
			};
		};

		cdc_reset_ctrl {
			cdc_reset_sleep: cdc_reset_sleep {
				config {
					drive-strength = <2>;
				};
			};

			cdc_reset_active: cdc_reset_active {
				config {
					drive-strength = <2>;
					bias-disable;
				};
			};
		};

		i2c_9 {
			i2c_9_sleep: i2c_9_sleep {
				config {
					bias-disable;
				};
			};

			i2c_9_bitbang: i2c_9_bitbang {
				config {
					bias-disable;
				};
			};
		};
	};

	sound-9335 {
		qcom,audio-routing =
			"AIF4 VI", "MCLK",
			"RX_BIAS", "MCLK",
			"MADINPUT", "MCLK",
			"DMIC0", "MIC BIAS1",
			"MIC BIAS1", "Digital Mic1",
			"DMIC3", "MIC BIAS3",
			"MIC BIAS3", "Digital Mic2",
			"EAR Switch", "EAR";

		/delete-property/ qcom,us-euro-gpios;
		/delete-property/ qcom,hph-en0-gpio;
		/delete-property/ qcom,hph-en1-gpio;

		qcom,wsa-max-devs = <0>;

		/delete-property/ qcom,wsa-devs;
		/delete-property/ qcom,wsa-aux-dev-prefix;

		/delete-node/ hph_en0_gpio;
		/delete-node/ hph_en1_gpio;
	};

	/delete-node/ sound-tavil;

	/delete-node/ wcd_us_euro_gpio;
};

&slim_aud {
	tasha_codec {
		swr_master {
			wsa881x@20170211 {
				status = "disabled";
			};

			wsa881x@20170212 {
				status = "disabled";
			};

			wsa881x@21170213 {
				status = "disabled";
			};

			wsa881x@21170214 {
				status = "disabled";
			};
		};
	};

	/delete-node/ wcd934x_cdc;
};
