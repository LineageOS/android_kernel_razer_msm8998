&soc {
	tlmm: pinctrl@03400000 {
		blsp1_uart3: blsp1_uart3 {
			blsp1_uart3_rxcts_sleep: blsp1_uart3_rxcts_sleep {
				config {
					bias-pull-up;
				};
			};

			blsp1_uart3_rfr_sleep: blsp1_uart3_rfr_sleep {
				config {
					bias-pull-up;
				};
			};
		};
	};
};

&soc {
	spi@c175000 { /* BLSP1 QUP1 */
		status = "ok";
	};

	spi@c179000 { /* BLSP1 QUP5 */
		status = "okay";
	};

	spi@c1ba000 { /* BLSP2 QUP6 */
		status = "ok";
	};

	qcom,pcie@01c00000 {
		/delete-property/ pinctrl-names;
		/delete-property/ pinctrl-0;
		/delete-property/ pinctrl-1;
		/delete-property/ wake-gpio;
	};
};

